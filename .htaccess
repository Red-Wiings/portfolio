# Redirections https mickaelarabian.fr
RewriteEngine On
RewriteCond %{HTTPS} !=on
RewriteCond %{HTTP_HOST} ^(?:www\.)?mickaelarabian\.fr(?:.*)$ [nc]
RewriteRule ^(.*)$ https://%{HTTP_HOST}%{REQUEST_URI} [redirect=301,L]

RewriteCond %{HTTP_HOST} ^mickaelarabian.fr$
RewriteRule ^(.*) https://www.mickaelarabian.fr/$1 [QSA,L,R=301]


RewriteCond %{REQUEST_URI} index\.html
RewriteRule .* https://www.mickaelarabian.fr [R=301,L]



<IfModule mod_headers.c>
  # Mise en cache des images et autres fichier statiques pour un mois
  <FilesMatch ".(ico|jpe?g|png|gif|swf|flv|css|js|gz|pdf)$">
   Header set Cache-Control "max-age=60480000
  </FilesMatch>



  # Désactive la mise en cache pour les fichier HTML PHP et CGI
  <FilesMatch ".(php|html|cgi)$">
   Header unset Cache-Control
  </FilesMatch>
</IfModule>
